msgctxt "QObject|"
msgid "Write Error"
msgstr "Ошибка записи"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Ошибка при попытке записи данных.\n"
"Детали ошибки: %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Ошибка при работе с разделами"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Ошибка при попытке получить handle тома.\n"
"Детали ошибки: %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Ошибка блокировки"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Ошибка при попытке наложить блокировку на том.\n"
"Детали ошибки: %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Ошибка разблокировки"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Ошибка при попытке снять блокировку с тома.\n"
"Детали ошибки %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Ошибка отмонтирования"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Ошибка при попытке отмонтировать том\n"
"Детали ошибки %1: %2"
