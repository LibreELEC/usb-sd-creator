msgid "Download complete, syncing file..."
msgstr "Загрузка завершена, синхронизация файлов"

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Загрузка завершена, проверка контрольной суммы"

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Загрузка завершена, контрольная сумма проверена"

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Загрузка завершена, ошибка контрольной суммы"

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "%1 секунд осталось - %2% шага %3"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Загрузка отменена"

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Катлог для сохранения файла образа"

msgctxt "Creator|"
msgid "Error"
msgstr "Ошибка"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"Файл \n"
"%1/%2\n"
"уже существует."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Вы хотите перезаписать?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "Файл уже существует"

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Не удалось открыть файл для записи!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Открыть файл образа"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed "
"image (*.img);;All files (*.*)"
msgstr ""
"Образ сжатый gz (*img.gz);;Образ сжатый zip (*img.zip);;Несжатый образ "
"(*.img);;Все файлы (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Запись отменена"

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Подтвердите запись"

#, qt-format
msgctxt "Creator|"
msgid "Cannot unmount partititons on device %1"
msgstr "Не удается отключить разделы на устройстве %1"

