msgid "&Write"
msgstr "Записать"

msgctxt "Creator|"
msgid "&Help"
msgstr "Помощь"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Выбрать файл"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Показать всё"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Отмена"

