msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD Creator"

msgctxt "Creator|"
msgid "Write image"
msgstr "Skriv bildefil"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "Velg minnebrikken eller minnekortet ditt"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Last ned eller velg en lokal bildefil"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Løs ut enhet"

msgctxt "Creator|"
msgid "Load device"
msgstr "Last enhet"

#~ msgctxt "Creator|"
#~ msgid "Eject  Load   Remove"
#~ msgstr "Løs ut Last Fjern"

msgctxt "Creator|"
msgid "Select project"
msgstr "Velg prosjekt"

msgctxt "Creator|"
msgid "Select image"
msgstr "Velg bildefil"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"Rot-tillatelser er påkrevd for å skrive bildefil.\n"
"Kjør applikasjonen med sudo."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Versjon: %1\n"
"Byggdato: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Denne programvaren er laget med kjærlighet og sluppet"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "under GPLv2, og bruker tidligere arbeid fra RasPlex."

msgctxt "Creator|"
msgid "For license, credits and history, please read:"
msgstr "For lisens, kreditt og historie, vennligst les:"

msgctxt "Creator|"
msgid "If you enjoy using LibreELEC please consider a"
msgstr "Hvis du nyter å bruke LibreELEC, vennligst vurder en"

msgctxt "Creator|"
msgid "donation to support the project."
msgstr "donasjon for å støtte prosjektet."

msgctxt "Creator|"
msgid "Click the logo below or donate"
msgstr "Klikk på logoen under, eller doner"

msgctxt "Creator|"
msgid "using Paypal to:"
msgstr "ved hjelp av Paypal for:"

msgctxt "Creator|"
msgid "[Stable]"
msgstr "[Stabil]"

msgctxt "Creator|"
msgid "[Alpha]"
msgstr "[Alfa]"

msgctxt "Creator|"
msgid "[Beta]"
msgstr "[Beta]"

msgctxt "Creator|"
msgid "Download complete, syncing file..."
msgstr "Nedlasting komplett, synkroniserer fil..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Nedlasting komplett, verifiserer sjekksum."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Nedlasting komplett, sjekksum ok."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Nedlasting komplett, sjekksum ikke ok."

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "%1 sekunder gjenstår - %2% på %3"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Nedlasting avbrutt."

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Mappe for å lagre bildefilen"

msgctxt "Creator|"
msgid "Error"
msgstr "Feil"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"Fil\n"
"%1/%2\n"
"eksisterer allerede."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Vil du overskrive?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "Filen eksisterer allerede."

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Åpning av fil for skriving feilet!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Åpne bildefil"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed "
"image (*.img);;All files (*.*)"
msgstr ""
"Komprimert gz bilde (*img.gz);;Komprimert zip bilde (*img.zip);;Ukomprimert "
"bilde (*.img);;Alle filer (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Skriving avbrutt."

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Bekreft skriving"

