msgid "&Download"
msgstr "Lataa"

msgctxt "Creator|"
msgid "&About"
msgstr "Tietoja"

msgctxt "Creator|"
msgid "&Write"
msgstr "Kirjoita"

msgctxt "Creator|"
msgid "&Help"
msgstr "Apua"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Valitse tiedosto"

msgctxt "Creator|"
msgid "&Show all"
msgstr "Näytä kaikki"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Peruuta"

msgctxt "Creator|"
msgid "&Visit Website"
msgstr "Käy web-sivulla"

