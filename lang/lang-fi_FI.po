msgctxt "QObject|"
msgid "Write Error"
msgstr "Kirjoitusvirhe"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Virhe kirjoitettaessa tietoa kahvaan.\n"
"Virhe %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Levyvirhe"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Tapahtui virhe kahvaa varattaessa.\n"
"Virhe %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Lukitusvirhe"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Virhe yritettäessä levyn lukitsemista.\n"
"Virhe %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Virhe avattaessa lukkoa"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Virhe yritettäessä levyn lukituksen poistoa.\n"
"Virhe %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Virhe irroittaessa"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Virhe yritettäessä levyn irroitusta.\n"
"Virhe %1: %2"
