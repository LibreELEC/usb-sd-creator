msgctxt "QObject|"
msgid "Write Error"
msgstr "Skrivefejl"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"En fejl opstod ved forsøg på at skrive data til handle.\n"
"Fejl %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Volume Fejl"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"En fejl opstod ved forsøg på at få et \"handle\" på volume'et.\n"
"Error %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Låse Fejl"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"En fejl opstod ved forsøg på at låse volume.\n"
"Fejl %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Aflåse Fejl"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"En fejl opstod ved forsøg på at aflåse volume.\n"
"Fejl %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Afmonteringsfejl"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"En fejl opstod ved forsøg på at afmontere volume.\n"
"Fejl %1: %2"
