msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD Creator"

msgctxt "Creator|"
msgid "Write image"
msgstr "Skriv image"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "Vælg dit USB-stick eller SD-kort"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Hent eller vælg en lokal image-fil"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Skub ud"

msgctxt "Creator|"
msgid "Load device"
msgstr "Indlæs enhed"

msgctxt "Creator|"
msgid "Eject  Load   Remove"
msgstr "Fjern Eject Load"

msgctxt "Creator|"
msgid "Select project"
msgstr "Vælg projekt"

msgctxt "Creator|"
msgid "Select image"
msgstr "Vælg image"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"Rod privilegier nødvendige for at skrive image.\n"
"Kør applikation med sudo."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Version: %1\n"
"Build dato: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Denne software blev lavet med kærlighed og udgivet"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "under GPLv2, benytter tidligere arbejde fra RasPlex."

