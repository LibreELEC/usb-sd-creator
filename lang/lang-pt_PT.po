msgid "Select project"
msgstr "Selecionar projeto"

msgctxt "Creator|"
msgid "Select image"
msgstr "Selecionar imagem"

msgctxt "QObject|"
msgid "Write Error"
msgstr "Erro de escrita"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Ocorreu um erro ao tentar gravar dados no handle.\n"
"Erro %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Erro no volume"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Ocorreu um erro ao tentar obter um handle no volume.\n"
"Erro %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Erro de bloqueio"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Ocorreu um erro ao tentar bloquear o volume.\n"
"Erro %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Erro de desbloqueio"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Ocorreu um erro ao tentar desbloquear o volume.\n"
"Erro %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Erro a desmontar"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Ocorreu um erro ao tentar desmontar o volume.\n"
"Erro %1: %2"
