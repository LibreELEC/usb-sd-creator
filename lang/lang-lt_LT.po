msgid "&Help"
msgstr "&Pagalba"

msgctxt "Creator|"
msgid "Select &file"
msgstr "Pasirinkti &failą"

msgctxt "Creator|"
msgid "&Show all"
msgstr "&Rodyti viską"

msgctxt "Creator|"
msgid "Cance&l"
msgstr "Atšau&kti"

msgctxt "Creator|"
msgid "&Visit Website"
msgstr "Ap%lankykite svetainę"

#, qt-format
msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator %1 is available."
msgstr "Prieinamas LibreELEC USB-SD Creator %1."

msgctxt "Creator|"
msgid "Select project"
msgstr "Pasirinkite projektą"

msgctxt "Creator|"
msgid "Select image"
msgstr "Pasirinkite atvaizdą"

msgctxt "QObject|"
msgid "Write Error"
msgstr "Įrašymo klaida"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to write data to handle.\n"
"Error %1: %2"
msgstr ""
"Įvyko klaida mėginant įrašyti duomenis.\n"
"Klaida %1: %2"

msgctxt "QObject|"
msgid "Volume Error"
msgstr "Tomo klaida"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to get a handle on the volume.\n"
"Error %1: %2"
msgstr ""
"Įvyko klaida mėginant gauti tomo valdymą.\n"
"Klaida %1: %2"

msgctxt "QObject|"
msgid "Lock Error"
msgstr "Užrakinimo klaida"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to lock the volume.\n"
"Error %1: %2"
msgstr ""
"Įvyko klaida mėginant užrakinti tomą.\n"
"Klaida %1: %2"

msgctxt "QObject|"
msgid "Unlock Error"
msgstr "Atrakinimo klaida"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to unlock the volume.\n"
"Error %1: %2"
msgstr ""
"Įvyko klaida mėginant atrakinti tomą.\n"
"Klaida %1: %2"

msgctxt "QObject|"
msgid "Dismount Error"
msgstr "Atjungimo klaida"

#, qt-format
msgctxt "QObject|"
msgid ""
"An error occurred when attempting to dismount the volume.\n"
"Error %1: %2"
msgstr ""
"Įvyko klaida mėginant atjungti tomą.\n"
"Klaida %1: %2"
