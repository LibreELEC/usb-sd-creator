msgctxt "Creator|"
msgid "LibreELEC USB-SD Creator"
msgstr "LibreELEC USB-SD Creator"

msgctxt "Creator|"
msgid "Write image"
msgstr "Įrašykite atvaizdą"

msgctxt "Creator|"
msgid "Select your USB stick or SD card"
msgstr "Pasirinkite USB atmintinę arba SD kortelę"

msgctxt "Creator|"
msgid "Download or select a local image file"
msgstr "Atsisiųskite arba pasirinkite lokalų atvaizdo failą"

msgctxt "Creator|"
msgid "Eject device"
msgstr "Išimti įrenginį"

msgctxt "Creator|"
msgid "Load device"
msgstr "Įkelti įrenginį"

msgctxt "Creator|"
msgid ""
"Root privileges required to write image.\n"
"Run application with sudo."
msgstr ""
"Norint įrašyti atvaizdą reikalingos root teisės.\n"
"Paleiskite programą su sudo komanda."

#, qt-format
msgctxt "Creator|"
msgid ""
"Version: %1\n"
"Build date: %2"
msgstr ""
"Versija: %1\n"
"Darinio data: %2"

msgctxt "Creator|"
msgid "This software was created with love and released"
msgstr "Ši programinė įranga buvo sukurta su meile ir išleista"

msgctxt "Creator|"
msgid "under GPLv2, using earlier work from RasPlex."
msgstr "su GPLv2 licencija, panaudojus ankstesnį RasPlex darbą. "

msgctxt "Creator|"
msgid "For license, credits and history, please read:"
msgstr "Jei reikia licencijos, kūrėjų sąrašo ar istorijos, perskaitykite:"

msgctxt "Creator|"
msgid "If you enjoy using LibreELEC please consider a"
msgstr "Jei patinka naudotis LibreELEC, apsvarstykite"

msgctxt "Creator|"
msgid "donation to support the project."
msgstr "galimybę paaukoti projekto palaikymui."

msgctxt "Creator|"
msgid "Click the logo below or donate"
msgstr "Paspauskite ant žemiau esančio logotipo norėdami paaukoti"

msgctxt "Creator|"
msgid "using Paypal to:"
msgstr "naudojantis Paypal:"

msgctxt "Creator|"
msgid "[Stable]"
msgstr "[Stabili]"

msgctxt "Creator|"
msgid "[Alpha]"
msgstr "[Alpha]"

msgctxt "Creator|"
msgid "[Beta]"
msgstr "[Beta]"

msgctxt "Creator|"
msgid "Download complete, syncing file..."
msgstr "Atsiuntimas baigtas, sinchronizuojamas failas..."

msgctxt "Creator|"
msgid "Download complete, verifying checksum..."
msgstr "Atsiuntimas baigtas, tikrinama kontrolinė suma..."

msgctxt "Creator|"
msgid "Download complete, checksum ok."
msgstr "Atsiuntimas baigtas, kontrolinė suma teisinga."

msgctxt "Creator|"
msgid "Download complete, checksum not ok."
msgstr "Atsiuntimas baigtas, kontrolinė suma klaidinga."

#, qt-format
msgctxt "Creator|"
msgid "%1 seconds remaining - %2% at %3"
msgstr "Liko %1 sekundžių - %2% baigta %3 sparta"

msgctxt "Creator|"
msgid "Download canceled."
msgstr "Atsiuntimas atšauktas."

msgctxt "Creator|"
msgid "Directory to store image file"
msgstr "Katalogas, kuriame saugoti atvaizdo failą"

msgctxt "Creator|"
msgid "Error"
msgstr "Klaida"

#, qt-format
msgctxt "Creator|"
msgid ""
"File \n"
"%1/%2\n"
"already exist."
msgstr ""
"Failas \n"
"%1/%2\n"
"jau egzituoja."

msgctxt "Creator|"
msgid "Do you want to overwrite?"
msgstr "Ar norite perrašyti?"

msgctxt "Creator|"
msgid "File already exists."
msgstr "Failas jau egzistuoja."

msgctxt "Creator|"
msgid "Failed to open file for writing!"
msgstr "Nepavyko atidaryti failo įrašymui!"

msgctxt "Creator|"
msgid "Open image file"
msgstr "Atidaryti atvaizdo failą"

msgctxt "Creator|"
msgid ""
"Compressed gz image (*img.gz);;Compressed zip image (*img.zip);;Uncompressed"
" image (*.img);;All files (*.*)"
msgstr ""
"Suglaudintas gz atvaizdas (*img.gz);;Suglaudintas zip atvaizdas "
"(*img.zip);;Nesuglaudintas atvaizdas (*.img);;Visi failai (*.*)"

msgctxt "Creator|"
msgid "Writing canceled."
msgstr "Įrašymas atšauktas."

msgctxt "Creator|"
msgid "Confirm write"
msgstr "Patvirtinkite įrašymą"

#, qt-format
msgctxt "Creator|"
msgid "Cannot unmount partititons on device %1"
msgstr "Neįmanoma atjungti skaidinių įrenginyje %1"

#, qt-format
msgctxt "Creator|"
msgid "Not enough space on %1 [%2 < %3]"
msgstr "Nepakanka vietos įrenginyje %1 [%2 < %3]"

msgctxt "Creator|"
msgid "Syncing file system..."
msgstr "Sinchronizuojama failų sistema..."

msgctxt "Creator|"
msgid "Writing done!"
msgstr "Įrašymas baigtas!"

